{
  "name": "nodejsstrategydesingpattern",
  "version": "1.0.0",
  "private": true,
  "index": "./server/bin/www",
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/bin/www",
    "dev": "SET NODE_ENV=development & npm-run-all build server",
    "prod": "SET NODE_ENV=production & npm-run-all build server",
    "transpile": "babel ./server --out-dir dist-server --copy-files",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon",
    "fetch:tasks": "node ./dist-server/fetch-tasks.js"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongoose": "^6.0.13",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "uuid": "^8.3.2",
    "@babel/runtime": "^7.14.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-async-to-generator": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "nodemon": "^2.0.12"
  }
}